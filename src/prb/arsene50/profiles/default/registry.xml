<?xml version="1.0"?>
<registry>

    <record name="prb.arsene50.currentoffer">
        <field type="plone.registry.field.TextLine">
            <title>CurrentOffer</title>
            <description>Url of webservice from arsene50</description>
        </field>
        <value>http://api.arsene50.be/currentOffer</value>
    </record>

    <record name="prb.arsene50.nextoffer" remove="True" />

    <record name="prb.arsene50.agenda_api_token">
        <field type="plone.registry.field.TextLine">
            <title>Agenda API Token</title>
            <description>Token Agenda API from api.brussels</description>
        </field>
        <value></value>
    </record>

</registry>
